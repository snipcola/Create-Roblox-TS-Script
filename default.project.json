{
  "name": "roblox-ts",
  "globIgnorePaths": ["**/package.json", "**/tsconfig.json"],
  "tree": {
    "$path": "dist",
    "include": {
      "$path": "include",
      "node_modules": {
        "$className": "Folder",
        "@rbxts": {
          "$path": "node_modules/@rbxts"
        }
      }
    }
  }
}
